<h1>會員登入</h1>

<%= link_to "返回列表", root_path %>

<%= form_with(model: @user, url: login_path, method: "post", local: true) do |form| %>

<div class="fields">
    <%= form.label :email ,"E-MAIL" %>
    <%= form.text_field :email %>
</div>

<div class="fields">
    <%= form.label :password ,"密碼" %>
    <%= form.password_field :password %>
</div>


<%= form.submit "登入" %>
<% end %>